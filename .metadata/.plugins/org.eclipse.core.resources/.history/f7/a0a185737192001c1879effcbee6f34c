$(document).ready(function(){
		$('.signin-btn').click(function(){
			$('body').css('opacity','0.4')
			$('.signin-popup').css({"display":"block","opacitiy":"1"});

		});
		
		 $('.signup-btn').click(function(){
			$('body').css('opacity','0.4')
			$('.signup-popup').css({"display":"block","opacitiy":"1"});
		});
});